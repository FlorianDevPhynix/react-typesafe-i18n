{
  "name": "react-safe-i18n",
  "version": "1.1.1",
  "description": "",
  "keywords": [
    "react",
    "typescript",
    "typesafe",
    "internationalization",
    "i18n",
    "translation",
    "localization",
    "internationalization library",
    "localization library",
    "localization engine"
  ],
  "license": "MIT",
  "author": "Florian Stemmler",
  "repository": {
    "type": "git",
    "url": "https://github.com/FlorianDevPhynix/react-typesafe-i18n"
  },
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./formatters": {
      "types": "./dist/formatters.d.ts",
      "require": "./dist/formatters.js",
      "import": "./dist/formatters.mjs"
    },
    "./internal": {
      "types": "./dist/internal.d.ts",
      "require": "./dist/internal.js",
      "import": "./dist/internal.mjs"
    }
  },
  "files": [
    "dist/*",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "build": "vite build",
    "lint": "eslint ./src --ext ts,tsx && npm run check",
    "check": "tsc --noEmit",
    "format": "prettier --config ../.prettierrc --write ./src",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "release": "pnpm run build && bumpp --no-push",
    "trace": "tsc --generateTrace trace && pnpm --package=@typescript/analyze-trace dlx analyze-trace ./trace"
  },
  "peerDependencies": {
    "react": "18.x"
  },
  "dependencies": {
    "react": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "^16.0.12",
    "@types/react": "18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/ui": "^0.34.1",
    "bumpp": "^9.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-tsdoc": "^0.2.17",
    "prettier": "3.0.2",
    "typesafe-i18n": "5.26.2",
    "typesafe-utils": "^1.16.2",
    "typescript": "5.0.2",
    "utility-types": "^3.10.0",
    "valibot": "^0.28.0",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.3",
    "vitest": "^0.31.4"
  }
}